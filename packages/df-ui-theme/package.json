{
  "name": "@dataflor-ag/df-ui-theme",
  "version": "1.4.0",
  "description": "DATAflor Web Application UI Theme",
  "homepage": "https://github.com/dataflor-ag/webstyleguide#readme",
  "license": "UNLICENSED",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@dataflor-ag"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dataflor-ag/webstyleguide.git"
  },
  "scripts": {
    "build": "tsc && vite build",
    "prepublishOnly": "npm run build",
    "lint": "tsc && eslint . --ext .js,.ts,.jsx,.tsx --fix"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@mui/x-data-grid": "^7.2.0",
    "@mui/x-tree-view": "^7.5.1",
    "@mui/x-data-grid-pro": "^6.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3"
  },
  "bugs": {
    "url": "https://github.com/dataflor-ag/webstyleguide/issues"
  }
}
